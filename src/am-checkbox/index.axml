<view class="am-checkbox {{className}}" style="height:{{size}}px; width: {{size}}px;">
  <checkbox
    class="am-checkbox-value"
    value="{{value}}"
    checked="{{checked}}"
    disabled="{{disabled}}"
    onChange="onChange"
    id="{{id}}"
    />
  <view class="am-checkbox-synthetic"></view>
</view>